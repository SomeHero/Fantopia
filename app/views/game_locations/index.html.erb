<h1>Listing game_locations</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Street address</th>
    <th>City</th>
    <th>State abbreviation</th>
    <th>Zip</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @game_locations.each do |game_location| %>
  <tr>
    <td><%= game_location.name %></td>
    <td><%= game_location.street_address %></td>
    <td><%= game_location.city %></td>
    <td><%= game_location.state_abbreviation %></td>
    <td><%= game_location.zip %></td>
    <td><%= game_location.latitude %></td>
    <td><%= game_location.longitude %></td>
    <td><%= link_to 'Show', game_location %></td>
    <td><%= link_to 'Edit', edit_game_location_path(game_location) %></td>
    <td><%= link_to 'Destroy', game_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Game location', new_game_location_path %>
